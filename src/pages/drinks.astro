<Layout title="Drinks Menu" subtitle="Refresh Your Day">
    <style>
        /* General Section Styling */
        .menu-section {
            text-align: center;
            margin-bottom: 30px;
            padding: 20px;
            border-radius: 10px;
            background: rgba(0, 0, 0, 0.7);
        }

        h2 {
            font-size: 2rem;
            color: rgb(4, 128, 52);
            font-weight: bold;
            padding: 10px 20px;
            margin: 0 auto 20px;
            background: rgba(0, 0, 0, 0.8);
            border-radius: 10px;
            width: fit-content;
        }

        /* Intro Image */
        .intro-image {
            margin-bottom: 20px;
        }

        .intro-image img {
            width: 100%;
            max-width: 1200px;
            height: auto;
            border-radius: 10px;
            display: block;
            margin: 0 auto;
            transition: transform 0.3s ease;
        }

        .intro-image img:hover {
            transform: scale(1.05);
        }

        /* Menu List */
        .menu-list {
            display: grid;
            grid-template-columns: repeat(auto-fit, minmax(250px, 1fr));
            gap: 15px;
            padding: 15px;
            border-radius: 10px;
            margin-top: 10px;
            width: 100%;
            max-width: 1200px;
            margin-left: auto;
            margin-right: auto;
            transition: transform 0.3s ease;
        }

        .menu-list:hover {
            transform: scale(1.02);
        }

        .menu-item {
            font-size: 1.2rem;
            font-weight: bold;
            color: white;
            padding: 8px 10px;
            background: rgba(255, 255, 255, 0.1);
            border-radius: 5px;
            max-width: 300px;
            display: flex;
            flex-direction: column; /* Stack name/price and button vertically */
            align-items: stretch; /* Stretch children to full width */
        }

        .item-details {
            display: flex;
            justify-content: space-between;
            align-items: center;
            width: 100%;
            margin-bottom: 5px; /* Space between name/price and button */
        }

        .item-details span:first-child {
            flex: 1; /* Name takes available space */
            text-align: left;
            overflow: hidden; /* Prevent long names from breaking layout */
            text-overflow: ellipsis; /* Add ellipsis for long names */
            white-space: nowrap; /* Keep name on one line */
        }

        .item-details span:nth-child(2) {
            min-width: 100px; /* Uniform width for price */
            text-align: right;
        }

        /* Order Button Styling */
        .order-btn {
            background: rgba(57, 109, 14, 0.8);
            color: white;
            border: none;
            padding: 6px 12px;
            border-radius: 5px;
            font-weight: bold;
            font-size: 1rem;
            cursor: pointer;
            transition: background 0.3s ease;
            width: 100%; /* Full width within menu-item */
        }

        .order-btn:hover {
            background: rgba(21, 102, 71, 1);
        }

        /* Category-Specific Backgrounds */
        .vodka { background: rgba(82, 24, 22, 0.8); }
        .whiskey { background: rgba(105, 87, 96, 0.8); }
        .beer { background: rgba(57, 109, 14, 0.8); }
        .gin { background: rgba(21, 102, 71, 0.8); }
        .cocktails { background: rgba(255, 87, 34, 0.8); }
        .spirits { background: rgba(255, 215, 0, 0.8); }
        .soft-drinks { background: rgba(70, 130, 180, 0.8); }
        .rum-creams { background: rgba(139, 69, 19, 0.8); }
        .wines { background: rgba(128, 0, 128, 0.8); }
        .liqueur { background: rgba(0, 128, 128, 0.8); }

        /* Image Row */
        .image-row {
            display: flex;
            flex-wrap: nowrap; /* Keeps images horizontal on all sizes */
            gap: 15px;
            width: 100%;
            max-width: 1200px; /* Consistent with other pages */
            padding: 10px;
            margin: 0 auto 20px; /* Centered with bottom margin */
            justify-content: space-between;
        }

        .image-row img {
            width: calc(33.33% - 10px); /* Three images, accounting for gap */
            height: auto;
            border-radius: 10px;
            transition: transform 0.3s ease;
            object-fit: cover;
        }

        .image-row img:hover {
            transform: scale(1.05);
        }

        /* Responsive Adjustments */
        @media (max-width: 768px) {
            .intro-image img {
                max-width: 100%;
            }

            .menu-list {
                grid-template-columns: repeat(auto-fit, minmax(200px, 1fr));
            }

            .menu-item {
                font-size: 1.1rem;
                padding: 6px 8px;
            }

            .item-details span:nth-child(2) {
                min-width: 90px; /* Slightly smaller for tablets */
            }

            .image-row img {
                height: 150px; /* Fixed height for tablets */
            }

            .order-btn {
                padding: 5px 10px;
                font-size: 0.9rem;
            }
        }

        @media (max-width: 480px) {
            .menu-section {
                padding: 15px;
            }

            .menu-list {
                grid-template-columns: 1fr; /* Single column on mobile */
            }

            .menu-item {
                font-size: 1rem;
                padding: 5px 6px;
                max-width: none; /* Full width on mobile */
            }

            .item-details span:nth-child(2) {
                min-width: 80px; /* Compact but uniform on mobile */
            }

            .image-row img {
                height: 100px; /* Fixed height for mobile */
            }

            .order-btn {
                padding: 4px 8px;
                font-size: 0.85rem;
            }
        }
    </style>

